<h1>Registro de servicios</h1>

<% form_for :service, @service, :url=> {:action => "create"} do |f| %>
  <%= render :partial => 'form' %>

  <p><font color=red>(*) Los campos marcados son de llenado obligatorio.</font> </p>

<table cellspacing="10" width="380">
<tr align="center">
  <td colspan="2"><%= submit_tag "Registrar" %></td></tr>

<% end %>
<tr align="center">
  <td colspan="2"><%= link_to 'Regresar', :action => 'list' %>  </td>
</tr>
</table>
