<h1>Agregar contactoo de servicios</h1>

<table cellspacing="10">
<% form_tag :action => 'create' do %>
  <%= render :partial => 'form' %>

  <tr><td colspan="2"><font color=red>(*) Los campos marcados son de llenado obligatorio.</font> </td></tr>

  <tr align="center"><td colspan="2"><%= submit_tag "Registrar" %></td></tr>
  <tr align="center"><td colspan="2"><%= link_to 'Regresar',:action=>'list' %></td></tr>
<% end %>
</table>
